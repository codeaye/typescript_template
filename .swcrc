{
	"minify": true,
	"exclude": [".*.spec.ts$", ".*.test.ts$"],
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"decorators": true
		},
		"target": "es2020",
		"paths": {
			"#lib/*": ["./lib/*"]
		},
		"baseUrl": "./src",
		"minify": {
			"compress": {
				"unused": true
			},
			"mangle": true
		}
	},
	"module": {
		"type": "commonjs"
	}
}
